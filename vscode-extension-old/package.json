{
  "name": "firebase-vscode",
  "displayName": "firebase-vscode",
  "description": "Firebase VS Code Extension",
  "version": "0.0.1",
  "publisher": "firebase",
  "repository": "https://github.com/firebase/firebase-tools",
  "engines": {
    "vscode": "^1.51.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "helloworld.helloWorld",
        "title": "Hello World"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./"
  },
  "devDependencies": {
    "@types/node": "^18.11.11",
    "@types/vscode": "^1.73.1",
    "tslint": "^6.1.3",
    "typescript": "^4.9.3"
  }
}
